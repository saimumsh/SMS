@{
    ViewData["Title"] = "Home Page";
}
<body>
    <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
        <div class="pt-3 pb-2 mb-3 border-bottom">
            <ul class="nav nav-tabs" id="myTab" role="tablist">
                <li class="nav-item" role="presentation">
                    <button class="nav-link active" id="home-tab" data-bs-toggle="tab" data-bs-target="#class" type="button" role="tab" aria-controls="home" aria-selected="true">Class</button>
                </li>
                <li class="nav-item" role="presentation">
                    <button class="nav-link" id="profile-tab" data-bs-toggle="tab" data-bs-target="#teacher" type="button" role="tab" aria-controls="profile" aria-selected="false">Teachers</button>
                </li>
                <li class="nav-item" role="presentation">
                    <button class="nav-link" id="contact-tab" data-bs-toggle="tab" data-bs-target="#student" type="button" role="tab" aria-controls="contact" aria-selected="false">Student</button>
                </li>
            </ul>
        </div>
        <div class="tab-content" id="myTabContent">
            <div class="tab-pane fade show active" id="class" role="tabpanel" aria-labelledby="class-tab">
                <p>This is the Home tab content.</p>
            </div>
            <div class="tab-pane fade" id="teacher" role="tabpanel" aria-labelledby="teacher-tab">
                <p>This is the Profile tab content.</p>
            </div>
            <div class="tab-pane fade" id="student" role="tabpanel" aria-labelledby="student-tab">
                <p>This is the Contact tab content.</p>
            </div>
        </div>
    </main>

    <!-- Bootstrap JS and Popper.js --> 
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.min.js"></script>
</body>

